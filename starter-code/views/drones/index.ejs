<table class="table">
  <thead>
    <tr>
      <th class="text-center" scope="col"><h2>Name</h2></th>
      <th class="text-center" scope="col"><h2>Propellers</h2></th>
      <th class="text-center" scope="col"><h2>MaxSpeed</h2></th>
    </tr>
  </thead>
  <tbody>
    <% drones.forEach((drone) => { %>
      <tr>
        <td><%= drone.name %></td>
        <td class="text-center"><%= drone.propellers %></td>
        <td class="text-center"><%= drone.maxSpeed %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
<div class="text-center">
  <a href="drones/new" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Create New</a>
</div>